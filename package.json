{
  "name": "web-3tier",
  "version": "0.1.0",
  "bin": {
    "web-3tier": "bin/web-3tier.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --fix \"./**/*.{ts,json}\"",
    "test": "jest"
  },
  "pre-commit": [
    "build",
    "test",
    "lint"
  ],
  "devDependencies": {
    "@aws-cdk/assert": "1.4.0",
    "@types/jest": "24.0.18",
    "@types/node": "8.10.51",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "eslint": "6.2.2",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-autoscaling": "1.4.0",
    "@aws-cdk/aws-ec2": "1.4.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "1.4.0",
    "@aws-cdk/aws-rds": "1.4.0",
    "@aws-cdk/core": "1.4.0",
    "aws-cdk": "1.4.0",
    "source-map-support": "0.5.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  }
}
